<?xml version='1.0' encoding='utf-8'?>
<widget id="com.lape.proyectoawug3" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>LostNFound</name>
    <description>
        Aplicaci√≥n Proyecto 3,Grupo 7
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Alexandra Barron, Eva Barbero, Laura Gonzalez, Pau Enrech
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="res/icon/android/LNF_icon_ldpi.png" />
        <icon density="mdpi" src="res/icon/android/LNF_icon_mdpi.png" />
        <icon density="hdpi" src="res/icon/android/LNF_icon_hdpi.png" />
        <icon density="xhdpi" src="res/icon/android/LNF_icon_xhdpi.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <engine name="android" spec="^6.3.0" />
    <plugin name="cordova-plugin-camera" spec="^4.0.2" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.3" />
    <plugin name="cordova-plugin-crop" spec="git+https://github.com/obeza/cordova-plugin-crop-with-ratio.git" />
</widget>
