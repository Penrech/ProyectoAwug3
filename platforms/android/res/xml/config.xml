<?xml version='1.0' encoding='utf-8'?>
<widget id="com.lape.proyectoawug3" version="1.0.0" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <feature name="Camera">
        <param name="android-package" value="org.apache.cordova.camera.CameraLauncher" />
    </feature>
    <feature name="Whitelist">
        <param name="android-package" value="org.apache.cordova.whitelist.WhitelistPlugin" />
        <param name="onload" value="true" />
    </feature>
    <feature name="CropPlugin">
        <param name="android-package" value="com.jeduan.crop.CropPlugin" />
    </feature>
    <name>LostNFound</name>
    <description>
        Aplicaci√≥n Proyecto 3,Grupo 7
    </description>
    <author email="dev@cordova.apache.org" href="http://cordova.io">
        Alexandra Barron, Eva Barbero, Laura Gonzalez, Pau Enrech
    </author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <allow-intent href="market:*" />
    <icon density="ldpi" src="res/icon/android/LNF_icon_ldpi.png" />
    <icon density="mdpi" src="res/icon/android/LNF_icon_mdpi.png" />
    <icon density="hdpi" src="res/icon/android/LNF_icon_hdpi.png" />
    <icon density="xhdpi" src="res/icon/android/LNF_icon_xhdpi.png" />
    <preference name="loglevel" value="DEBUG" />
</widget>
